{
  "name": "lesson0_webpack",
  "version": "1.0.0",
  "description": "webpack lesson",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode development",
    "build_": "webpack --mode production ./src/index.js --output ./dist/main.js",
    "dev_": "webpack --mode development ./src/index.js --output ./dev/main.js",
    "build__": "webpack --mode production --module-bind js=babel-loader --output ./dist/main.js",
    "dev__": "webpack --mode development --module-bind js=babel-loader --output ./dev/main.js",
    "start":"webpack-dev-server --mode development --open"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "babel-loader": "^8.0.4",
    "css-loader": "^1.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.5",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {}
}
